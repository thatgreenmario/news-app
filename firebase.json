{
  "hosting": {
    "public": "dist/news-app",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [ {
      "source": "https://dnr-news-app.web.app/api/v1/topics",
      "destination": "https://brutalist.report/api/v1/topics",
      "type": 301
    }
    ]
  }
}

